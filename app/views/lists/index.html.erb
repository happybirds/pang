	<div class="main">
		<!-- <div class="banner">
		</div> -->
		<div class="schedule">
			<!-- <button type="button" class='btn'>path</button> -->
			<a href="<%= path_path%>" class='btn'>Bus</a>
			<h4> <%= "#{Date.today.strftime("%Y/%m/%d, %a")}，#{Date.today.strftime('%Y')}的第#{Date.today.strftime('%j')}天" %>

			</h4>

			<div class="alldate">
			<% 1..5.times do |i| %>
						<div class="date<%= i+1 %>">
						<span><%= changeW(i+1) %></span>
								<% @schedules.where(date: i+1).each do |s|%>
									<ul style="border-color: <%= s.bgcolor %>;margin-top: <%= s.margin %>px">
									<li style="background-color: <%= s.bgcolor %>"><%= s.start%>-<%= s.end %></li>
									<li><%= s.title%></li>
									<li><%= s.address%></li>
									<li><%= s.classroom%></li>
								</ul>
								<% end %>
					</div>
			<% end %>
		</div>
		</div>
    <div class="path">
      <h1>Notice</h1>
      <div class="notice"><%=@notice%></div>
    </div>
